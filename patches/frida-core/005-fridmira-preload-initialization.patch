 			public async HashTable<string, Variant> query_system_parameters (Cancellable? cancellable) throws GLib.Error {
-				return yield parent.host_session.query_system_parameters (cancellable);
+				// 当 FridMira 混淆模式启用时，直接构造返回值避免 HostSession 指数退避
+				// When FridMira obfuscation mode is enabled, construct return value directly to avoid HostSession exponential backoff
+				if (Frida.FridMira.is_globally_enabled() &&
+				    Frida.FridMiraRpcProtocolEnhancer.is_enhanced_mode_enabled()) {
+
+					// 构造最小系统参数返回值，避免 HostSession 指数退避
+					// Construct minimal system parameter return value to avoid HostSession exponential backoff
+					var params = new HashTable<string, Variant> (null, null);
+
+					// 添加基本系统信息
+					// Add basic system information
+					params["platform"] = new Variant.string (Gum.Process.get_current_platform ().to_string ());
+					params["arch"] = new Variant.string (Gum.Process.get_current_arch ().to_string ());
+					params["name"] = new Variant.string ("FridMira Server");
+					params["version"] = new Variant.string ("16.7.19");
+
+					// 添加 FridMira 协议信息到系统参数中
+					// Add FridMira protocol information to system parameters
+					params["_fridmira_protocol_id"] =
+						new Variant.string (Frida.FridMiraRpcProtocolEnhancer.get_protocol_id (null));
+					params["_fridmira_call_command"] =
+						new Variant.string (Frida.FridMiraRpcProtocolEnhancer.get_call_command (null));
+					params["_fridmira_ok_response"] =
+						new Variant.string (Frida.FridMiraRpcProtocolEnhancer.get_ok_response (null));
+					params["_fridmira_enhanced_mode"] =
+						new Variant.boolean (true);
+					params["_fridmira_version"] =
+						new Variant.string ("16.7.19");
+
+					return params;   // 直接返回，避免指数退避，0-1ms 内完成
+				}
+
+				// 混淆关闭时走原始实现
+				// Use original implementation when obfuscation is disabled
+				var params = yield parent.host_session.query_system_parameters (cancellable);
+
+				// 即使在标准模式下也添加FridMira状态信息
+				// Add FridMira status information even in standard mode
+				params["_fridmira_enhanced_mode"] = new Variant.boolean (false);
+				params["_fridmira_version"] = new Variant.string ("16.7.19");
+
+				return params;
 			}
 
